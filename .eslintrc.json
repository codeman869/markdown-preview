{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
        ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures" : {
            "jsx": true
        }
    },
    "settings": {
        "react": {
            "version": "16.5"
        }
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unexpected-multiline": "error"
    }
}